#include <iostream>
 
using namespace std;
 
int main() {
	long n,i,x,temp,flag =0,count = 0;
	cin>>n>>x;
	for(i=0;i<n;i++){
	    cin>>temp;
	    if(x<temp){
	        flag += 1;
	        if(flag > 1){
	            cout<<count;
	            break;
	        }
	    }
	    else count += 1;
	    if(i== n-1 && flag <=1){
	            cout<<count;
	        }
	}
    
}
 
Language: C++
