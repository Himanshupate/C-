#include <iostream>
 
using namespace std;
 
int main() {
	int c,t,n,i,cost,count;
	cin>>t;
	while(t--){
	    cin>>n>>cost;
	    long a[n];
	    for(i=0;i<n;i++){
	        cin>>a[i];
	    }
	    count = 1;
	    c = a[0];
	    for(i=0;i<n;i++){
	        if(a[i]>c){
	            c = a[i];
	            count++;
	        }
	    }
	    c = cost*count;
	   cout<<c<<endl;
	    
	}
	 
}
Language: C++
