#include<iostream>
using namespace std;
int checkprime(int n);
int main(){
    int i,N;
    cin>>N;
    for(i=2;i<N;i++){
        
        if(checkprime(i)){
        cout<<i<<" ";
        }
    }
} 
int checkprime(int n){
    for(int i=2;i<n;i++){
        if(n%i == 0){
            return 0;
        }
        
    }
    return 1;
}
Language: C++14
